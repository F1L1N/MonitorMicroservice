@{
    ViewBag.Title = "Все анализы";
}

@model MonitorMicroservice.Models.Analyzes.Analyse
<a asp-action="CreateBloodBio">Добавить биохимический анализ крови</a>
<table class="table">
    <tr>
        <th>ID анализа</th>
        <th>ID пациента</th>
        <th>Операции</th>
    </tr>

    @foreach (var item in Model.AnalyseBloodBio)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.PatientId</td>
            <td>
                <a asp-action="Details" asp-controller="Analyse" asp-route-id="@item.Id">Подробнее</a> |
                <a asp-action="Edit" asp-controller="Analyse" asp-route-id="@item.Id">Изменить</a> |
                <a asp-action="Delete" asp-controller="Analyse" asp-route-id="@item.Id">Удалить</a>
            </td>
        </tr>
    }
</table>

