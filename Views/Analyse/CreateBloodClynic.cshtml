@model MonitorMicroservice.Models.Analyzes.AnalyseBloodClynic
@{
    ViewBag.Title = "Добавление биохимичекого анализа крови";
}
<form asp-action="CreateBloodClynic" asp-controller="Analyse">
    <div class="form-group">
        <label asp-for="PatientId" class="control-label">ID пациента</label>
        <input type="text" asp-for="PatientId" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Hemoglobin" class="control-label">Гемоглобин</label>
        <input type="text" asp-for="Hemoglobin" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Erythrocyte" class="control-label">Эритроциты</label>
        <input type="text" asp-for="Erythrocyte" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Leukocyte" class="control-label">Лейкоциты</label>
        <input type="text" asp-for="Leukocyte" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Platelet" class="control-label">Тромбоциты</label>
        <input type="text" asp-for="Platelet" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Отправить" class="btn btn-default" />
    </div>
</form>